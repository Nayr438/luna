{
	"authors": [
		"ech0"
	],
	"copyright": "Copyright Â© 2023, ech0",
	"dependencies": {
		"archive": "~>0.7.1",
		"toml": "~>2.0.1"
	},
	"dflags": [
		"-Oz",
		"-link-defaultlib-shared",
		"-function-sections",
		"-data-sections"
	],
	"lflags": [
		"-Wl,--gc-sections",
		"-Wl,--strip-all",
		"-flto=full"
	],
	"license": "BSD 3-clause",
	"postBuildCommands": [
		"strip ./libluna.so"
	],
	"name": "luna",
	"description": "luna package manager",
	"configurations": [
		{
			"name": "luna",
			"targetType": "executable",
			"mainSourceFile": "source/main.d"
		},
		{
			"name": "libluna",
			"targetType": "dynamicLibrary",
			"targetName": "luna",
			"mainSourceFile": "",
			"excludedSourceFiles": [
				"source/main.d"
			]
		}
	]
}